{
  "author": "Conner Ohnesorge",
  "description": "This plugin allows for the automatic tagging of note content within the metadata of your notes.",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "keywords": [
	"obsidian",
	"plugin", 
	"tags",
	"openai",
	"api",
	"openai-api",
	"openai-gpt3",
	"openai-gpt3-api",
	"openai-gpt3-plugin",
	"openai-chatgpt", 
	"cohere", 
	"cohere-api",
	"cohere-plugin",
	"cohere-chatbot",
	"artificial-intelligence", 
	"ai",
	"typescript",
	"javascript",
	"svelte",
	"obsidian-svelte",
	"obsidian-svelte-plugin",
  ],
  "license": "MIT",
  "main": "main.js",
  "name": "obsidian-sample-plugin",
  "scripts": {
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "dev": "node esbuild.config.mjs",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "versioned": "node version-bump.mjs $npm_package_version $npm_package_version && git add manifest.json versions.json"
  },
  "version": "1.0.0"
}
